{
  "name": "gitis",
  "version": "0.0.0",
  "description": "A git-backed and YAML-based distributed issue tracker.",
  "main": "build/index.js",
  "bin": {
    "gitis": "build/cli.js",
    "gitis-dev": "source/cli-dev.js"
  },
  "directories": {
    "test": "tests",
    "lib": "source",
    "issues": "issues"
  },
  "scripts": {
