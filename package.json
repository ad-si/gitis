  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adius/gitis/issues"
  },
  "homepage": "https://github.com/adius/gitis#readme",
  "dependencies": {
    "babel": "^5.8.29",
    "fs-promise": "^0.3.1",
    "js-yaml": "^3.4.3",
    "nconf": "^0.8.2",
    "yargs": "^4.1.0"
  }
}
